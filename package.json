{
  "name": "airtable-api-generator",
  "version": "1.0.0",
  "description": "Description",
  "keywords": [],
  "repository": "https://github.com/rmkasendwa/devskiller-test-decision-maker",
  "license": "MIT",
  "main": "build/main/index.js",
  "module": "build/module/index.js",
  "typings": "build/main/index.d.ts",
  "files": [
    "build/main",
    "build/module",
    "!**/*.spec.*",
    "!**/*.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build:main": "tsc -p tsconfig.json",
    "build:module": "tsc -p tsconfig.module.json",
    "doc": "run-s doc:html && open-cli build/docs/index.html",
    "doc:html": "typedoc src/ --exclude **/*.spec.ts --target ES6 --mode file --out build/docs",
    "doc:json": "typedoc src/ --exclude **/*.spec.ts --target ES6 --mode file --json build/docs/typedoc.json",
    "doc:publish": "gh-pages -m \"[ci skip] Updates\" -d build/docs",
    "version": "standard-version",
    "start": "tsnd --inspect=9888 --ignore-watch node_modules --respawn --transpile-only src/generate.ts",
    "format": "prettier --write \"./**/*.{js,jsx,ts,tsx,html,htm,json,css,scss,md}\""
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js"
    ],
    "extends": "@istanbuljs/nyc-config-typescript"
  },
  "dependencies": {
    "@bitauth/libauth": "^1.19.1",
    "@infinite-debugger/rmk-js-extensions": "^0.1.0-alpha.4",
    "@infinite-debugger/rmk-utils": "^0.1.0-alpha.5",
    "axios": "^1.2.1",
    "base-64": "^1.0.0",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "lodash": "^4.17.21",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@ava/typescript": "^3.0.1",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/base-64": "^1.0.0",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.17",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "ava": "^5.1.0",
    "codecov": "^3.8.2",
    "cspell": "^6.17.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^4.4.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "fs-extra": "^11.1.0",
    "gh-pages": "^4.0.0",
    "globby": "^10.0.2",
    "import-sort-style-module": "^6.0.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "open-cli": "^7.1.0",
    "prettier": "^2.8.1",
    "prettier-plugin-import-sort": "0.0.7",
    "prettier-plugin-sort-json": "0.0.3",
    "standard-version": "^9.5.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typedoc": "^0.23.23",
    "typescript": "^4.9.4"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  }
}
