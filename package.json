{
  "name": "@theoremone-talent/airtable-client",
  "version": "1.0.0-alpha.125",
  "description": "Airtable client",
  "keywords": [
    "airtable",
    "airtable-client",
    "client",
    "api",
    "api-generator",
    "generator"
  ],
  "repository": "https://github.com/TheoremOne-Talent/airtable-client.git",
  "author": {
    "name": "Ronald M. Kasendwa",
    "email": "ronald.kasendwa@theoremone.co",
    "url": "https://github.com/rmkasendwa"
  },
  "main": "./index.js",
  "bin": {
    "airtable-client": "./run.js"
  },
  "scripts": {
    "prebuild": "rm -rf ./dist",
    "build": "npm run tsc",
    "postbuild": "cp -r package.json ./src/template-files ./dist && node ./scripts/postbuild",
    "doc": "run-s doc:html && open-cli build/docs/index.html",
    "doc:html": "typedoc src/ --exclude **/*.spec.ts --target ES6 --mode file --out build/docs",
    "doc:json": "typedoc src/ --exclude **/*.spec.ts --target ES6 --mode file --json build/docs/typedoc.json",
    "doc:publish": "gh-pages -m \"[ci skip] Updates\" -d build/docs",
    "eslint": "eslint --fix",
    "format": "prettier --write \"./**/*.{js,jsx,ts,tsx,html,htm,json,css,scss,md}\"",
    "prepare": "husky install",
    "publish": "npm run build && cd ./dist && npm publish --registry https://npm.pkg.github.com",
    "publish:dev": "npm run build && cd ./dist && npm publish --registry http://localhost:4873",
    "start": "tsnd --inspect=9892 --ignore-watch node_modules --respawn --transpile-only src/__tests__/run.test.ts",
    "tsc": "tsc --project tsconfig.compile.json",
    "version": "standard-version"
  },
  "dependencies": {
    "base-64": "^1.0.0",
    "dotenv": "^16.0.3",
    "fs-extra": "^11.1.0",
    "pluralize": "^8.0.0",
    "ts-node": "^10.9.1",
    "walk-sync": "^3.0.0"
  },
  "devDependencies": {
    "@infinite-debugger/axios-api-adapter": "^0.1.0-alpha.12",
    "@infinite-debugger/rmk-js-extensions": "^0.1.0-alpha.24",
    "@infinite-debugger/rmk-utils": "^0.1.0-alpha.9",
    "@tsed/ajv": "^7.9.0",
    "@tsed/common": "^7.9.0",
    "@tsed/core": "^7.9.0",
    "@tsed/di": "^7.9.0",
    "@tsed/exceptions": "^7.9.0",
    "@tsed/json-mapper": "^7.9.0",
    "@tsed/platform-express": "^7.9.0",
    "@tsed/schema": "^7.9.0",
    "@tsed/swagger": "^7.9.0",
    "@types/eslint": "^8.4.10",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.17",
    "@types/pluralize": "^0.0.29",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "ajv": "^8.11.2",
    "axios": "^1.2.2",
    "body-parser": "^1.20.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "date-fns": "^2.29.3",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^4.4.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "express": "^4.18.2",
    "hash-it": "^6.0.0",
    "husky": "^8.0.3",
    "import-sort-style-module": "^6.0.0",
    "lodash": "^4.17.21",
    "method-override": "^3.0.0",
    "prettier": "^2.8.1",
    "prettier-plugin-import-sort": "^0.0.7",
    "prettier-plugin-sort-json": "^0.0.3",
    "standard-version": "^9.5.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4",
    "zod": "^3.20.2"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  }
}
