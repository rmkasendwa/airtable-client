{
  "name": "airtable-client",
  "version": "1.0.0",
  "description": "Description",
  "keywords": [],
  "repository": "https://github.com/TheoremOne-Talent/airtable-api-generator",
  "scripts": {
    "prebuild": "rm -rf ./dist",
    "build": "npm run tsc",
    "postbuild": "cp -r package.json ./src/template-files ./dist",
    "doc": "run-s doc:html && open-cli build/docs/index.html",
    "doc:html": "typedoc src/ --exclude **/*.spec.ts --target ES6 --mode file --out build/docs",
    "doc:json": "typedoc src/ --exclude **/*.spec.ts --target ES6 --mode file --json build/docs/typedoc.json",
    "doc:publish": "gh-pages -m \"[ci skip] Updates\" -d build/docs",
    "format": "prettier --write \"./**/*.{js,jsx,ts,tsx,html,htm,json,css,scss,md}\"",
    "start": "tsnd --inspect=9888 --ignore-watch node_modules --respawn --transpile-only src/index.ts",
    "tsc": "tsc --project tsconfig.compile.json",
    "version": "standard-version"
  },
  "dependencies": {
    "@infinite-debugger/rmk-js-extensions": "^0.1.0-alpha.5",
    "@infinite-debugger/rmk-utils": "^0.1.0-alpha.5",
    "axios": "^1.2.1",
    "base-64": "^1.0.0",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "lodash": "^4.17.21",
    "walk-sync": "^3.0.0",
    "fs-extra": "^11.1.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@types/eslint": "^8.4.10",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.17",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^4.4.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "import-sort-style-module": "^6.0.0",
    "prettier": "^2.8.1",
    "prettier-plugin-import-sort": "0.0.7",
    "prettier-plugin-sort-json": "0.0.3",
    "standard-version": "^9.5.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  }
}
